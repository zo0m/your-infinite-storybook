{
    "extends": "./tsconfig.json",
    "compilerOptions": {
        "outDir": "../../../../dist/out-tsc",
        "declaration": true,
        "types": [],
        "target": "es5",
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true
    },
    "include": ["**/*.ts"],
    "exclude": ["jest.config.ts", "**/*.spec.ts", "**/*.test.ts"]
}
